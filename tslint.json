{
  "extends": [
    "tslint-config-airbnb",
    "tslint-eslint-rules"
  ],
  "rules": {
    "align": [
      false
    ],
    "no-duplicate-imports": false,
    "max-line-length": [
      false
    ],
    "triple-equals": false,
    "variable-name": false,
    "function-name": false,
    "ter-max-len": [
      true,
      100,
      2,
      {
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true
      }
    ]
  }
}
